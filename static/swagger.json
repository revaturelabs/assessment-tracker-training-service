            "description": "Successfully found Trainer(s)",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/getTrainerResponseBody"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/years?trainerId=1": {
      "parameters": [
        {
